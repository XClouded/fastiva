rm -f $1.jar
BASEDIR=$(dirname $0) 
OUT_DIR=$1.fastiva/jni
rm -rf $OUT_DIR
echo $BASEDIR/jpp -apk $BASEDIR/dex2jar $1 -out $OUT_DIR \
    -cp_jars $JPP_OUT_DIR/framework-jars -application \
    -excludePackages org/apache/http \
    -includePackages org/apache/http/entity/mime \
    -ignore_unresolved_ref \
    -corruptMethods $BASEDIR/corruptMethod.lst $2 $3 $4 $5 $6 && \
$BASEDIR/jpp -apk $BASEDIR/dex2jar $1 -out $OUT_DIR \
    -cp_jars $JPP_OUT_DIR/framework-jars -application \
    -excludePackages org/apache/http \
    -includePackages org/apache/http/entity/mime \
    -ignore_unresolved_ref \
    -corruptMethods $BASEDIR/corruptMethod.lst $2 $3 $4 $5 $6 && \
cp $BASEDIR/cod/Android.mk $1.fastiva/jni && \
cp $BASEDIR/cod/Application.mk $1.fastiva/jni && \
ndk-build -C $1.fastiva NDK_DEBUG=0 
echo === exit without Build so ===
# exit


